{
    total=total+170;
    JOptionPane.showMessageDialog(this,"Mushroom Biriyani Ordered of Rs.170");
}
if(jList1.isSelectedIndex(4)==true)
{
    total=total+180;
    JOptionPane.showMessageDialog(this,"Mixed Veg Biriyani Ordered of Rs.180");
}
if(jList1.isSelectedIndex(5)==true)
{
    total=total+190;
    JOptionPane.showMessageDialog(this,"Plain Biriyani Ordered of Rs.190");
}
if(jList1.isSelectedIndex(6)==true)
{
    total=total+200;
    JOptionPane.showMessageDialog(this,"Egg Biriyani Ordered of Rs.200");
}
if(jList1.isSelectedIndex(7)==true)
{
    total=total+220;
    JOptionPane.showMessageDialog(this,"Chicken Biriyani Ordered of Rs.220");
}
if(jList1.isSelectedIndex(8)==true)
{
    total=total+240;
    JOptionPane.showMessageDialog(this,"Chicken 65 Biriyani Ordered of Rs.240");
}
if(jList1.isSelectedIndex(9)==true)
{
    total=total+260;
