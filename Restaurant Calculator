}
if(jList1.isSelectedIndex(10)==true)
{
    total=total+280;
    JOptionPane.showMessageDialog(this,"Mutton Biriyani Ordered of Rs.280");
}
if(jList1.isSelectedIndex(11)==true)
{
    total=total+300;
    JOptionPane.showMessageDialog(this,"Fish Biriyani Ordered of Rs.300");
}
jTextField1.setText(Integer.toString(total));
